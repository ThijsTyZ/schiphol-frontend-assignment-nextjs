{
  "name": "schiphol-frontend-assignment-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mediamonks/prettier-config": "^1.0.1",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "router-path": "^2.2.1"
  },
  "devDependencies": {
    "@mediamonks/eslint-config": "^2.3.0",
    "@mediamonks/eslint-config-react": "^2.4.0",
    "@mediamonks/eslint-config-typescript": "^1.3.0",
    "@mediamonks/eslint-config-typescript-react": "^1.2.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "extends": [
          "@mediamonks/eslint-config"
        ]
      },
      {
        "files": [
          "*.jsx"
        ],
        "extends": [
          "@mediamonks/eslint-config",
          "@mediamonks/eslint-config-react"
        ]
      },
      {
        "files": [
          "*.ts"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        },
        "extends": [
          "@mediamonks/eslint-config",
          "@mediamonks/eslint-config-typescript"
        ]
      },
      {
        "files": [
          "*.tsx"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        },
        "extends": [
          "@mediamonks/eslint-config",
          "@mediamonks/eslint-config-react",
          "@mediamonks/eslint-config-typescript",
          "@mediamonks/eslint-config-typescript-react"
        ]
      }
    ]
  },
  "prettier": "@mediamonks/prettier-config"
}
